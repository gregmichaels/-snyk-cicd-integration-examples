  pipeline {
    agent any

stages {
        stage('Build') {
            steps {
                sh 'rm -rf build'
                sh 'mkdir build'
                sh 'touch build/todo-list.txt'
                sh 'echo "http://127.0.0.1:5500/index.html"'
              }
        }
        stage('Test') {
            steps {
              sh 'test -f build/todo-list.txt'
              }
        }
    }
}
